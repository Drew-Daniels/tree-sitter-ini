{
  "name": "ini",
  "rules": {
    "document": {
      "type": "REPEAT",
      "content": {
        "type": "SYMBOL",
        "name": "section"
      }
    },
    "section": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "section_name"
        },
        {
          "type": "STRING",
          "value": "\n"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "setting"
          }
        }
      ]
    },
    "section_name": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "["
        },
        {
          "type": "PATTERN",
          "value": "[^\\[\\]]+"
        },
        {
          "type": "STRING",
          "value": "]"
        }
      ]
    },
    "setting": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "setting_name"
        },
        {
          "type": "STRING",
          "value": "="
        },
        {
          "type": "SYMBOL",
          "name": "setting_value"
        },
        {
          "type": "STRING",
          "value": "\n"
        }
      ]
    },
    "setting_name": {
      "type": "PATTERN",
      "value": "[^=]+"
    },
    "setting_value": {
      "type": "PATTERN",
      "value": "[^\\n]+"
    },
    "comment": {
      "type": "TOKEN",
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "STRING",
            "value": "#"
          },
          {
            "type": "PATTERN",
            "value": ".*"
          }
        ]
      }
    }
  },
  "extras": [
    {
      "type": "SYMBOL",
      "name": "comment"
    },
    {
      "type": "PATTERN",
      "value": "\\s"
    }
  ],
  "conflicts": [],
  "externals": [],
  "inline": [],
  "supertypes": []
}

