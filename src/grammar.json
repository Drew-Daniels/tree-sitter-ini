{
  "name": "ini",
  "rules": {
    "document": {
      "type": "REPEAT",
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "CHOICE",
            "members": [
              {
                "type": "SYMBOL",
                "name": "section_name"
              },
              {
                "type": "SYMBOL",
                "name": "setting"
              },
              {
                "type": "SYMBOL",
                "name": "comment"
              }
            ]
          },
          {
            "type": "CHOICE",
            "members": [
              {
                "type": "PATTERN",
                "value": "\\n+"
              },
              {
                "type": "BLANK"
              }
            ]
          }
        ]
      }
    },
    "section_name": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "["
        },
        {
          "type": "PATTERN",
          "value": "[^\\[\\]\\n]+"
        },
        {
          "type": "STRING",
          "value": "]"
        }
      ]
    },
    "setting": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "setting_name"
        },
        {
          "type": "STRING",
          "value": "="
        },
        {
          "type": "SYMBOL",
          "name": "setting_value"
        }
      ]
    },
    "setting_name": {
      "type": "PATTERN",
      "value": "[^#=\\s\\[]+"
    },
    "setting_value": {
      "type": "PATTERN",
      "value": "[^#\\n]+"
    },
    "comment": {
      "type": "TOKEN",
      "content": {
        "type": "PREC",
        "value": -10,
        "content": {
          "type": "PATTERN",
          "value": "#.*"
        }
      }
    }
  },
  "extras": [
    {
      "type": "SYMBOL",
      "name": "comment"
    },
    {
      "type": "PATTERN",
      "value": "[ \\t]+"
    }
  ],
  "conflicts": [],
  "externals": [],
  "inline": [],
  "supertypes": []
}

